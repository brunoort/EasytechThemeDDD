@*
    This is the main dashboard layout that creates the AdminLTE look and feel
       It includes all of the relevant CSS and Js in this project and across CDNs around the web for optimal load times
       TODO: Use this layout in your views by either setting the "Layout" property
       or changing your  _ViewStart.cshtml to use this layout
*@
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Easytech SIS</title>

    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <link href="//fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="http://code.ionicframework.com/ionicons/2.0.0/css/ionicons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/AdminLTE/plugins/morris/morris.css" rel="stylesheet" type="text/css" /
    <link href="~/Scripts/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/AdminLTE/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/AdminLTE/AdminLTE.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/AdminLTE/skins/_all-skins.min.css" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/globalFunctions.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/Bootstrap-3.3.6/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/DataTables-1.10.12/css/dataTables.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/AutoFill-2.1.2/css/autoFill.bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/Buttons-1.2.1/css/buttons.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/ColReorder-1.3.2/css/colReorder.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/FixedColumns-3.2.2/css/fixedColumns.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/FixedHeader-3.1.2/css/fixedHeader.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/Responsive-2.1.0/css/responsive.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/RowReorder-1.1.2/css/rowReorder.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/Scroller-1.4.2/css/scroller.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/DatatablesBS/Select-1.2.0/css/select.bootstrap.css" />
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    @Scripts.Render("~/bundles/modernizr")

</head>
<body class="skin-blue">

    <div class="wrapper">

        @Html.Partial("_AdminTopMenu")
        <!-- Left side column. contains the logo and sidebar -->
        <!-- Load do menu dinâmico-->

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                @Html.Partial("_AdminBreadcrumb")
            </section>
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Versão</b> 1.0
            </div>
            <strong>Copyright &copy; 2016 <a href="http://www.easytech.com.br">Easytech</a>.</strong> Todos os direitos reservados.
        </footer>
    </div><!-- ./wrapper -->
    @* Bundles assume jquery 2.1.3 and Bootstrap 3 *@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/growlNotify")
    <!-- FastClick -->
    <script src='~/Scripts/AdminLTE/plugins/fastclick/fastclick.min.js'></script>
    <!-- AdminLTE App -->
    <script src="~/Scripts/AdminLTE/app.js" type="text/javascript"></script>
    <!-- Sparkline -->
    <script src="~/Scripts/AdminLTE/plugins/sparkline/jquery.sparkline.min.js" type="text/javascript"></script>
    <!-- jvectormap -->
    <script src="~/Scripts/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js" type="text/javascript"></script>
    <!-- daterangepicker -->
    <script src="~/Scripts/AdminLTE/plugins/daterangepicker/daterangepicker.js" type="text/javascript"></script>
    <!-- datepicker -->
    <script src="~/Scripts/AdminLTE/plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>
    <!-- iCheck -->
    <script src="~/Scripts/AdminLTE/plugins/iCheck/icheck.min.js" type="text/javascript"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="~/Scripts/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <!-- ChartJS 1.0.1 -->
    <script src="~/Scripts/AdminLTE/plugins/chartjs/Chart.min.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            $('.dropdown-toggle').dropdown();
        });
    </script>
    @RenderSection("scripts", required: false)
  

</body>
</html>
